# Local backend profile
SPRING_PROFILES_ACTIVE=postgres

# Neon PostgreSQL connection
SPRING_DATASOURCE_URL=jdbc:postgresql://YOUR_NEON_HOST:5432/neondb?sslmode=require
SPRING_DATASOURCE_USERNAME=neondb_owner
SPRING_DATASOURCE_PASSWORD=replace_me

# Optional: protect mutating board/widget APIs
APP_ADMIN_TOKEN=replace_me

# Optional: comma-separated CORS allowlist (used in deploy)
# APP_CORS_ALLOWED_ORIGINS=https://your-vercel-domain.vercel.app
