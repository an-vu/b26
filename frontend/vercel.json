{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://b26-backend.onrender.com/api/:path*"
    },
    {
      "source": "/actuator/:path*",
      "destination": "https://b26-backend.onrender.com/actuator/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
